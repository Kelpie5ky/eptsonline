0442\\u0432\\u0438\\u044f \\u0441 \\u0442\\u0430\\u043c\\u043e\\u0436\\u0435\\u043d\\u043d\\u043e\\u0439 \\u0441\\u043b\\u0443\\u0436\\u0431\\u043e\\u0439 \\u0431\\u0435\\u0440\\u0435\\u0442 \\u043d\\u0430 \\u0441\\u0435\\u0431\\u044f \\u0431\\u0440\\u043e\\u043a\\u0435\\u0440. \\u0420\\u0430\\u0431\\u043e\\u0442\\u0430 \\u0432\\u044b\\u043f\\u043e\\u043b\\u043d\\u044f\\u0435\\u0442\\u0441\\u044f \\u0432 2 \\u044d\\u0442\\u0430\\u043f\\u0430: \\u0434\\u043e\\u043a\\u0443\\u043c\\u0435\\u043d\\u0442\\u044b \\u043f\\u0440\\u0438\\u0441\\u044b\\u043b\\u0430\\u044e\\u0442\\u0441\\u044f \\u043d\\u0430 \\u043f\\u043e\\u0447\\u0442\\u0443, \\u0437\\u0430\\u0442\\u0435\\u043c \\u0431\\u0440\\u043e\\u043a\\u0435\\u0440 \\u043f\\u043e\\u0434\\u0433\\u043e\\u0442\\u0430\\u0432\\u043b\\u0438\\u0432\\u0430\\u0435\\u0442 \\u043d\\u0443\\u0436\\u043d\\u044b\\u0439 \\u043a\\u043e\\u043c\\u043f\\u043b\\u0435\\u043a\\u0442 \\u0434\\u043e\\u043a\\u0443\\u043c\\u0435\\u043d\\u0442\\u0430\\u0446\\u0438\\u0438 \\u0432 \\u0441\\u043e\\u043e\\u0442\\u0432\\u0435\\u0442\\u0441\\u0442\\u0432\\u0438\\u0438 \\u0441 \\u0442\\u0440\\u0435\\u0431\\u043e\\u0432\\u0430\\u043d\\u0438\\u044f\\u043c\\u0438 \\u043d\\u043e\\u0440\\u043c\\u0430\\u0442\\u0438\\u0432\\u043d\\u043e-\\u043f\\u0440\\u0430\\u0432\\u043e\\u0432\\u043e\\u0439 \\u0431\\u0430\\u0437\\u044b. \\u0417\\u0430\\u0442\\u0435\\u043c \\u043d\\u0430 \\u0440\\u0443\\u043a\\u0435 \\u0432\\u044b\\u0434\\u0430\\u0435\\u0442\\u0441\\u044f \\u0433\\u043e\\u0442\\u043e\\u0432\\u043e\\u0435 \\u0422\\u041f\\u041e, \\u043a\\u043e\\u043f\\u0438\\u044f \\u043e\\u0442\\u0441\\u044b\\u043b\\u0430\\u0435\\u0442\\u0441\\u044f \\u043f\\u043e \\u044d\\u043b\\u0435\\u043a\\u0442\\u0440\\u043e\\u043d\\u043d\\u043e\\u0439 \\u043f\\u043e\\u0447\\u0442\\u0435, \\u0434\\u043b\\u044f \\u043f\\u0435\\u0440\\u0435\\u0434\\u0430\\u0447\\u0438 \\u0432 \\u0421\\u042d\\u041f. \\u0423\\u0441\\u043b\\u0443\\u0433\\u0430 \\u043e\\u043f\\u043b\\u0430\\u0447\\u0438\\u0432\\u0430\\u0435\\u0442\\u0441\\u044f \\u043f\\u043e \\u0441\\u0443\\u0449\\u0435\\u0441\\u0442\\u0432\\u0443\\u044e\\u0449\\u0435\\u043c\\u0443 \\u043f\\u0440\\u0430\\u0439\\u0441\\u0443 \\u043a\\u043e\\u043c\\u043f\\u0430\\u043d\\u0438\\u0438.<br /><br />\\u0422\\u0430\\u043a \\u043a\\u0430\\u043a \\u043c\\u044b \\u044f\\u0432\\u043b\\u044f\\u0435\\u043c\\u0441\\u044f \\u0430\\u043a\\u043a\\u0440\\u0435\\u0434\\u0438\\u0442\\u043e\\u0432\\u0430\\u043d\\u043d\\u043e\\u0439 \\u043b\\u0430\\u0431\\u043e\\u0440\\u0430\\u0442\\u043e\\u0440\\u0438\\u0435\\u0439, \\u0442\\u043e \\u0438 \\u0432\\u043e\\u043f\\u0440\\u043e\\u0441\\u044b \\u043f\\u043e \\u043e\\u043f\\u043b\\u0430\\u0442\\u0435 \\u0443\\u0442\\u0438\\u043b\\u0438\\u0437\\u0430\\u0446\\u0438\\u043e\\u043d\\u043d\\u043e\\u0433\\u043e \\u0441\\u0431\\u043e\\u0440\\u0430 \\u043c\\u043e\\u0436\\u0435\\u043c \\u0440\\u0435\\u0448\\u0430\\u0442\\u044c \\u0432 \\u0441\\u0447\\u0438\\u0442\\u0430\\u043d\\u043d\\u044b\\u0435 \\u0434\\u043d\\u0438. \\u041c\\u044b \\u043f\\u0440\\u043e\\u0432\\u043e\\u0434\\u0438\\u043c \\u043e\\u043f\\u043b\\u0430\\u0442\\u044b \\u043d\\u0430\\u0448\\u0438\\u0445 \\u043a\\u043b\\u0438\\u0435\\u043d\\u0442\\u043e\\u0432 \\u0434\\u0430\\u0436\\u0435 \\u0432 \\u0442\\u0435\\u0445 \\u0441\\u043b\\u0443\\u0447\\u0430\\u044f\\u0445, \\u043a\\u043e\\u0433\\u0434\\u0430 \\u043a\\u0430\\u0436\\u0435\\u0442\\u0441\\u044f \\u0447\\u0442\\u043e \\u043e\\u043d \\u0441\\u043b\\u043e\\u0436\\u043d\\u044b\\u0439 \\u0438 \\u0442\\u0430\\u043c\\u043e\\u0436\\u043d\\u044f \\u043e\\u0442\\u043a\\u0430\\u0437\\u044b\\u0432\\u0430\\u0435\\u0442 \\u0432 \\u043f\\u0440\\u0438\\u0435\\u043c\\u0435 \\u0434\\u043e\\u043a\\u0443\\u043c\\u0435\\u043d\\u0442\\u043e\\u0432.\"}, {\"ty\": \"heading\", \"te\": \"\\u0427\\u0442\\u043e \\u0431\\u0443\\u0434\\u0435\\u0442, \\u0435\\u0441\\u043b\\u0438 \\u0443\\u0442\\u0438\\u043b\\u0438\\u0437\\u0430\\u0446\\u0438\\u043e\\u043d\\u043d\\u044b\\u0439 \\u0441\\u0431\\u043e\\u044